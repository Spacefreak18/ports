# description : sord lihgtweight C library for sorting RDF statements in memory 
# depends	 : serd

name=sord
version=0.16.4
release=1
source="https://download.drobilla.net/$name-$version.tar.bz2"

build() {
	cd $name-$version
    ./waf configure --prefix=/usr
    ./waf build
	./waf install --destdir=$PKG
}
